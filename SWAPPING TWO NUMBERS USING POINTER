#include <stdio.h>
int main ( )
{
    int a,b,temp;
    int *p1,*p2;
    printf("enter the first number:");
    scanf("%d",&a);
    printf("\n enter the second number:");
    scanf("%d",&b);
    p1=&a;
    p2=&b;
    printf("\n two number before swapping:%d,%d",*p1,*p2);
    temp=*p1;
    *p1=*p2;
    *p2=temp;
    printf("\n two number after swapping:%d,%d",*p1,*p2);
    return 0;
}
